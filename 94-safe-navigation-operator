public String getAccountBillingCityWithSafeNavigation(List<Account> accounts){
   // Code here
    List<String> billingCities = new List<String>();
    
    for(Account acc: accounts){
        if(acc.BillingCity != null){
            billingCities.add(acc.BillingCity.toUpperCase());
        } else {
            return null; // You might want to handle the case where BillingCity is null for any account
        }
    }
    
    return billingCities;
   //return accounts?.get(0)?.BillingCity?.toUpperCase();
}
List<Account> accounts = new List<Account>{
    new Account(Name='Acme', BillingCity='Mumbai'),
    new Account(Name='Dove', BillingCity='Delhi')
};

List<String> result = getAccountBillingCityWithSafeNavigation(accounts);
System.debug('Result: ' + result);
