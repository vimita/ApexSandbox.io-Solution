public Boolean ascendingOrder(Integer a, Integer b, Integer c) {
    //code here
    if(a>=b || b >= a || c >= b){
        return true;
    }
    else{
        return false;
    }
}

Boolean result1 = ascendingOrder(10, 10, 15);
System.debug(result1);
Boolean result2 = ascendingOrder(15, 14, 13);
System.debug(result2);